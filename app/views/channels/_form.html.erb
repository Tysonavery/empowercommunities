<% form_for(@channel) do |f| %>
  <%= f.error_messages %> 
  <table>
    <tr>
      <td><%= f.label :name %></td>
      <td><%= f.text_field :name %></td>
    </tr>
    <tr>
      <td><%= f.label :description %></td>
      <td><%= f.text_area :description %></td>
    </tr>
    <tr>
      <td></td>
      <td><%= f.submit 'Submit' %></td>
    </tr> 
  </table>
<% end %>